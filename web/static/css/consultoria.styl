#consultoria-container
	//padding-left 50px
	background red
	height 100%
	width 100%
	.consultoria-title
		background url('/images/consultoria.png') no-repeat
		background-size cover
		height 100%
		width 100%
		display flex
		align-items center
		font-family "BebasNeue", sans-serif
		div
			position absolute
			width 100%
			height 100%
			background rgba(0,0,0, 0.36)
			z-index 1
		p
			font-size 160px
			margin-left 50px
			color white
			z-index 2
	.whatWeDo-container
		height 100%
		width 100%
		background -webkit-linear-gradient(rgb(230,48,46), rgb(166,48,44));
		display flex
		align-items center
		
		div
			width 100%
			color white
		.whatWeDo-title
			margin-left 50px
			p
				font-size 34px
				font-weight bold
			.whatWeDo-title-line
				margin-top 10px
				border-bottom 3px solid white
				width 150px
		.whatWeDo-text
			margin-right 100px
			p
				font-size 18px
	.ourVision
		//overflow:auto
		height auto
		width 100%
		background-image url('/images/ourVision.png')
		background-attachment fixed
		background-size cover
		background-repeat repeat
		&-title
			padding-top 50px
			margin-left 50px
			font-size 100px
			font-family "BebasNeue", sans-serif
		&-title-line
			width 250px
			margin-top 10px
			margin-left 50px
			border-bottom 3px solid red
		&-image img
			margin-left 30%
			width 40%
		&-info
			display flex
			flex-wrap wrap
			justify-content space-around
			margin 0 50px
			&-item
				width 30%
				&-title
					font-family "BebasNeue", sans-serif
					font-size 60px
				&-text
					font-size 18px
	.clients
		height 100%
		width 100%
		background url('/images/clients.png') no-repeat
		background-size 100% 100%
		background-attachment fixed
		padding-bottom 50px
		&-title
			padding-top 50px
			margin-left 50px
			font-size 100px
			font-family "BebasNeue", sans-serif
		&-title-line
			width 250px
			margin-top 10px
			margin-left 50px
			border-bottom 3px solid red
	.consultoria-footer
		height 200px
		background -webkit-linear-gradient(rgb(230,48,46), rgb(166,48,44));
					
		
				
				
		

