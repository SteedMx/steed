<script type="text/javascript">//<![CDATA[
$(window).load(function(){
  $(window).scroll(function(){

    $("#parallax section div").each(function(){
      var marginTop = $(window).scrollTop()-$(this).parent().position().top + 200;
      if(screen.width < 690) {
        marginTop -= 100;
      }
      $(this).css('margin-top', marginTop);
    });

    $(".site-body section").each(function(index, element){
      var topPosition = -200 + $(window).scrollTop()/ (15 * (index + 1));
      if (index === 1) {
        var auxPosition = ($(window).scrollTop()/ (15 * (index + 1))) * 2.5;
        $(this).css('background-position', 'right ' + auxPosition + 'px, ' + '0px ' + topPosition + 'px');          
      } else if(index === 2) {
        $(this).css('background-position', 'right ' + 'bottom, ' + '0px ' + topPosition + 'px');
      } else {
        $(this).css('background-position', '0px ' + topPosition + 'px');          
      }
    });

  });
});
</script>
<script>
// Open and close menu
$('#be-quoted').click(function() {
  $(this).toggleClass('be-quoted-open be-quoted-close')
  $('#be-quoted-section').toggleClass('be-quoted-hidden be-quoted-visible')
})
</script>
